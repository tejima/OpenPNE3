packages: 
  yum:
    mysql-server: []
    nmap: []
commands:
  01_mysql:
    command: /etc/init.d/mysqld start
container_commands:
  01_cp:
    command: cp config/OpenPNE.yml.sample config/OpenPNE.yml
  02_cp:
    command: cp config/ProjectConfiguration.class.php.sample config/ProjectConfiguration.class.php
  03_install:
    command: ./symfony openpne:fast-install --dbms=mysql --dbuser=root --dbpassword= --dbhost=localhost --dbname=OPENPNE --internet